DECLARE @Relatorio TABLE
(
     Area                         VARCHAR(200) -- Area de Conhecimento
    ,Curso                        VARCHAR(200) -- Descrição do Curso
    ,Turno                        VARCHAR(200) -- Descricao do Turno
    ,Matric                       INT
    ,NaoRematric                  INT
    ,MatricEfetuada               INT 
    ,TotalCalourosMatriculados    INT 
    ,TotalMatricVeteranosCalouros INT
    ,DesempenhoOferta             DECIMAL(10,2)
    
)
INSERT INTO @Relatorio
SELECT  
	Area                                                   AS Area                        
	,Curso                                                 AS Curso                       
	,Turno                                                 AS Turno                       
	,SUM(Matric)                                           AS Matric                      
	,SUM(NaoRematric)                                      AS NaoRematric                 
	,(SUM(MatricEfetuada) + SUM(Regularizado))             AS MatricEfetuada              
	,SUM(TotalCalourosMatriculados)                        AS TotalCalourosMatriculados   
	,SUM(TotalCalourosMatriculados) + SUM(MatricEfetuada) + SUM(Regularizado) AS TotalMatricVeteranosCalouros
	,CASE WHEN ((SUM(TotalCalourosMatriculados) + SUM(Matric) &gt; 0) AND (SUM(Matric) &gt; 0)) THEN
         ROUND(CONVERT(NUMERIC(10,2),((CONVERT(NUMERIC(10,5),(SUM(TotalCalourosMatriculados) + SUM(Matric))/CONVERT(NUMERIC(10,5),SUM(Matric)))-1)*100)),3)
    ELSE 0 END  AS DesempenhoOferta	
 FROM @tabela
WHERE curso != 'AA'

GROUP BY Area 
        ,curso
        ,Turno

SELECT 
	--'UNIVAG'                                                 AS Area                        
	-- ,Curso                                                AS Curso                       
	tb.Turno                                                AS Turno                       
	,SUM(tb.Matric)                                          AS SemestreAnterior
	,SUM(NaoRematric)                                        AS NaoRematric 
	,CASE WHEN (SUM(tb.Matric) &gt; 0) THEN  
          CONVERT(NUMERIC(10,2), ((ROUND(CONVERT(NUMERIC(10,2), (SUM(tb.NaoRematric)) * 100),2) / 
           ROUND(CONVERT(NUMERIC(10,2), SUM(tb.Matric) * 100),2)) * 100)) 
    ELSE 0 END  AS PercEvasaoTurno 
  
,(Select SUM(rel.Matric)
	from @Relatorio rel) as TotalSemestreAnteriorArea 
,(Select SUM(rel.NaoRematric) 
	from @Relatorio rel) as TotalNaoMatriculadoArea
 
                                      
	,CASE WHEN ((SUM(tb.Matric) &gt; 0) AND (SUM(tb.NaoRematric) &gt; 0)) THEN
CONVERT(NUMERIC(10,2),((Select ROUND(CONVERT(NUMERIC(10,2), SUM(rel.NaoRematric)),2) from @Relatorio rel) / 
(Select ROUND(CONVERT(NUMERIC(10,2), SUM(rel.Matric)),2)      from @Relatorio rel) * 100))
     
      ELSE 0 END  AS DesempenhoOferta	

FROM @Relatorio tb

GROUP BY -- tb.Area   
         --tb.Curso                        
       tb.Turno  

ORDER BY -- tb.Area 
        --tb.Curso                        
        tb.Turno  

