                    JOIN DBSecretariaAcademica.dbo.GradeConsepe
                      ON DBSecretariaAcademica.dbo.GradeConsepe.IdGradeConsepe = DBRelatorio.dbo.AlunoRelatorio.IdGradeConsepe
                    JOIN DBComum.dbo.Gpa
                      ON DBComum.dbo.Gpa.IdGpa = DBSecretariaAcademica.dbo.GradeConsepe.IdGpa
                   WHERE DBRelatorio.dbo.AlunoRelatorio.IdCampus               = @IdCampus
                     AND DBRelatorio.dbo.AlunoRelatorio.IdPeriodoLetivo        = @idPeriodoAnterior
                     AND DBRelatorio.dbo.AlunoRelatorio.SituacaoAcademicaAtivo = 1
                     AND DBRelatorio.dbo.AlunoRelatorio.IdAluno                = DBSecretariaAcademica.dbo.UvwConsultaAlunoSemestre.IdAluno                 
AND EXISTS (SELECT 1 FROM DBSecretariaAcademica.dbo.UvwConsultaAlunoSemestre ALS
                                     JOIN DBSecretariaAcademica.dbo.TipoAcessoIES
                                       ON DBSecretariaAcademica.dbo.TipoAcessoIES.IdTipoAcessoIES = ALS.IdTipoAcessoIES
                                    WHERE ALS.IdCampus               = @IdCampus
                                      AND ALS.IdPeriodoLetivo        = @IdPeriodoLetivo
                                      AND ALS.SituacaoAcademicaAtivo = 1
                                      AND ALS.IdAluno                = DBRelatorio.dbo.AlunoRelatorio.IdAluno)) 
   GROUP BY  DBComum.dbo.Gpa.Descricao
           	,DBSecretariaAcademica.dbo.UvwConsultaAlunoSemestre.CursoDescricao
		    ,DBSecretariaAcademica.dbo.UvwConsultaAlunoSemestre.TurnoCursoDescricao 
   ORDER BY  DBComum.dbo.Gpa.Descricao
			,DBSecretariaAcademica.dbo.UvwConsultaAlunoSemestre.CursoDescricao
			,DBSecretariaAcademica.dbo.UvwConsultaAlunoSemestre.TurnoCursoDescricao

