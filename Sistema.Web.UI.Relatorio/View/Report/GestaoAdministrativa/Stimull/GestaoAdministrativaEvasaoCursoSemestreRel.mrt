DECLARE @Relatorio TABLE
(
     Area                VARCHAR(200) 
    ,Curso               VARCHAR(200) 
    ,Turno               VARCHAR(200) 
    ,SemestreAnterior1   INT         
    ,NaoRematric1        INT         
    ,SemestreAnterior2   INT         
    ,NaoRematric2        INT         
    ,SemestreAnterior3   INT         
    ,NaoRematric3        INT         
    ,SemestreAnterior4   INT         
    ,NaoRematric4        INT         
    ,SemestreAnterior5   INT         
    ,NaoRematric5        INT         
    ,SemestreAnterior6   INT         
    ,NaoRematric6        INT         
    ,SemestreAnterior7   INT         
    ,NaoRematric7        INT         
    ,SemestreAnterior8   INT         
    ,NaoRematric8        INT         
    ,SemestreAnterior9   INT         
    ,NaoRematric9        INT         
    ,SemestreAnterior10  INT         
    ,NaoRematric10       INT         
    ,SemestreAnterior11  INT         
    ,NaoRematric11       INT         
    ,SemestreAnterior12  INT         
    ,NaoRematric12       INT   
)

INSERT INTO @Relatorio
SELECT  
	Area                                                  AS Area                        
	,Curso                                                 AS Curso                       
	,Turno                                                 AS Turno    
	,SUM(SemestreAnterior1)                                AS SemestreAnterior1
	,SUM(NaoRematric1)                                     AS NaoRematric1
	,SUM(SemestreAnterior2)                                AS SemestreAnterior2
	,SUM(NaoRematric2)                                     AS NaoRematric2
	,SUM(SemestreAnterior3)                                AS SemestreAnterior3
	,SUM(NaoRematric3)                                     AS NaoRematric3
	,SUM(SemestreAnterior4)                                AS SemestreAnterior4
	,SUM(NaoRematric4)                                     AS NaoRematric4
	,SUM(SemestreAnterior5)                                AS SemestreAnterior5
	,SUM(NaoRematric5)                                     AS NaoRematric5
	,SUM(SemestreAnterior6)                                AS SemestreAnterior6
	,SUM(NaoRematric6)                                     AS NaoRematric6
	,SUM(SemestreAnterior7)                                AS SemestreAnterior7
	,SUM(NaoRematric7)                                     AS NaoRematric7
	,SUM(SemestreAnterior8)                                AS SemestreAnterior8
	,SUM(NaoRematric8)                                     AS NaoRematric8
	,SUM(SemestreAnterior9)                                AS SemestreAnterior9
	,SUM(NaoRematric9)                                     AS NaoRematric9
	,SUM(SemestreAnterior10)                                AS SemestreAnterior10
	,SUM(NaoRematric10)                                     AS NaoRematric10
	,SUM(SemestreAnterior11)                                AS SemestreAnterior11
	,SUM(NaoRematric11)                                     AS NaoRematric11
	,SUM(SemestreAnterior12)                                AS SemestreAnterior12
	,SUM(NaoRematric12)                                     AS NaoRematric12

	FROM @tabela
--WHERE curso != 'AA'

GROUP BY Area 
        ,curso
        ,Turno

SELECT 
	tb.Area                                                 AS Area                        
	,tb.Curso                                                AS Curso                       
	,tb.Turno                                                AS Turno                       
	,SUM(tb.SemestreAnterior1)                               AS SemestreAnterior1
	,SUM(tb.NaoRematric1)                                    AS NaoRematric1
