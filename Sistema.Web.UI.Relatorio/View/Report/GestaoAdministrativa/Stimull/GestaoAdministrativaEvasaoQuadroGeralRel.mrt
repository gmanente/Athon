DECLARE @Relatorio TABLE
(
     Area                         VARCHAR(200) -- Area de Conhecimento
    ,Curso                        VARCHAR(200) -- Descrição do Curso
    ,Turno                        VARCHAR(200) -- Descricao do Turno
    ,Matric                       INT
    ,NaoRematric                  INT
    ,MatricEfetuada               INT 
    ,TotalCalourosMatriculados    INT 
    ,TotalMatricVeteranosCalouros INT
    ,DesempenhoOferta             DECIMAL(10,2)
    
)
INSERT INTO @Relatorio
SELECT  
	Area                                                  AS Area                        
	,Curso                                                 AS Curso                       
	,Turno                                                 AS Turno                       
	,SUM(Matric)                                           AS Matric                      
	,SUM(NaoRematric) - ((SUM(Formado) + SUM(Desistente) + Sum(Abandono))) AS NaoRematric                 
	,SUM(MatricEfetuada)                                   AS MatricEfetuada              
	,SUM(TotalCalourosMatriculados)                        AS TotalCalourosMatriculados   
	,SUM(TotalCalourosMatriculados) + SUM(MatricEfetuada)  AS TotalMatricVeteranosCalouros
	,CASE WHEN ((SUM(TotalCalourosMatriculados) + SUM(Matric) &gt; 0) AND (SUM(Matric) &gt; 0)) THEN
         ROUND(CONVERT(NUMERIC(10,2),((CONVERT(NUMERIC(10,5),(SUM(TotalCalourosMatriculados) + SUM(Matric))/CONVERT(NUMERIC(10,5),SUM(Matric)))-1)*100)),3)
    ELSE 0 END  AS DesempenhoOferta	
 FROM @tabela
WHERE curso != 'AA'

GROUP BY Area 
        ,curso
        ,Turno

SELECT 
	tb.Curso AS Curso 
	,CASE WHEN ((SUM(tb.Matric) &gt; 0) AND (SUM(tb.NaoRematric) &gt; 0)) THEN
CONVERT(NUMERIC(10,2),((Select ROUND(CONVERT(NUMERIC(10,2), SUM(rel.NaoRematric)),2) from @Relatorio rel where rel.Curso = tb.Curso) / 
(Select ROUND(CONVERT(NUMERIC(10,2), SUM(rel.Matric)),2)      from @Relatorio rel where rel.Curso = tb.Curso) * 100))
     
      ELSE 0 END  AS EvasaoArea	
    ,case when (CASE WHEN ((SUM(tb.Matric) &gt; 0) AND (SUM(tb.NaoRematric) &gt; 0)) THEN
CONVERT(NUMERIC(10,2),((Select ROUND(CONVERT(NUMERIC(10,2), SUM(rel.NaoRematric)),2) from @Relatorio rel where rel.Curso = tb.Curso) / 
(Select ROUND(CONVERT(NUMERIC(10,2), SUM(rel.Matric)),2)      from @Relatorio rel where rel.Curso = tb.Curso) * 100))
     
      ELSE 0 END) &lt; 9 THEN 1  
      when (CASE WHEN ((SUM(tb.Matric) &gt; 0) AND (SUM(tb.NaoRematric) &gt; 0)) THEN
CONVERT(NUMERIC(10,2),((Select ROUND(CONVERT(NUMERIC(10,2), SUM(rel.NaoRematric)),2) from @Relatorio rel where rel.Curso = tb.Curso) / 
(Select ROUND(CONVERT(NUMERIC(10,2), SUM(rel.Matric)),2)      from @Relatorio rel where rel.Curso = tb.Curso) * 100))
     
      ELSE 0 END) &lt; 10 THEN 2 ELSE 3 END Tipo

FROM @Relatorio tb

GROUP BY tb.Curso   


ORDER BY CASE WHEN ((SUM(tb.Matric) &gt; 0) AND (SUM(tb.NaoRematric) &gt; 0)) THEN
CONVERT(NUMERIC(10,2),((Select ROUND(CONVERT(NUMERIC(10,2), SUM(rel.NaoRematric)),2) from @Relatorio rel where rel.Curso = tb.Curso) / 
(Select ROUND(CONVERT(NUMERIC(10,2), SUM(rel.Matric)),2)      from @Relatorio rel where rel.Curso = tb.Curso) * 100))
     
ELSE 0 END asc 







</SqlCommand>
