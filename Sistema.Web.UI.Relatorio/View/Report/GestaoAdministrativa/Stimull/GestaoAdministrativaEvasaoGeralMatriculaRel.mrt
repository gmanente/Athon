SELECT 
	tb.Area                                                 AS Area                        
	-- ,Curso                                                AS Curso                       
	,tb.Turno                                                AS Turno                       
	,SUM(tb.Matric)                                          AS SemestreAnterior
	--,SUM(NaoRematric)                                     AS NaoRematric                    
	,SUM(tb.MatricEfetuada)                                  AS Veteranos                                                   
	,SUM(tb.TotalCalourosMatriculados)                       AS Calouros                                        
	,SUM(tb.TotalCalourosMatriculados) + SUM(tb.MatricEfetuada)  AS TotalTurno  
	,(Select SUM(rel.TotalCalourosMatriculados) + SUM(rel.MatricEfetuada)
       from @Relatorio rel
       where rel.area = tb.Area       
         --AND rel.Curso = tb.Curso
         ) as TotalArea
,(Select SUM(rel.Matric) 
	from @Relatorio rel
       where rel.area = tb.Area
         --AND rel.Curso = tb.Curso
         ) as TotalSemestreAnterior  
                                      
	,CASE WHEN ((SUM(tb.TotalCalourosMatriculados) + SUM(tb.Matric) &gt; 0) AND (SUM(tb.MatricEfetuada) &gt; 0)) THEN
ROUND(CONVERT(NUMERIC(10,2),((CONVERT(NUMERIC(10,5),((Select SUM(rel.TotalCalourosMatriculados) + SUM(rel.MatricEfetuada)
	from @Relatorio rel
                                                                where rel.area = tb.Area
                                                                  --AND rel.Curso = tb.Curso
))/CONVERT(NUMERIC(10,5),((Select SUM(rel.Matric) 
	from @Relatorio rel
                                                                                                                    where rel.area = tb.Area
                                                                                                                    --AND rel.Curso = tb.Curso
                                                                                                                    ))))-1)*100)),3)
    ELSE 0 END  AS DesempenhoOferta	

FROM @Relatorio tb

GROUP BY tb.Area   
        --,tb.Curso                        
        ,tb.Turno  

ORDER BY tb.Area 
        --,tb.Curso                        
        ,tb.Turno  

