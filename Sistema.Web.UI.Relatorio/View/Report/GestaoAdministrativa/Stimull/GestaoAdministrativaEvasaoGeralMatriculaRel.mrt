---------------------------------------------------------------------------------------------------
INSERT INTO @tabela
SELECT
	DBComum.dbo.Gpa.Descricao
	,DBSecretariaAcademica.dbo.UvwConsultaAlunoSemestre.CursoDescricao
	,DBSecretariaAcademica.dbo.UvwConsultaAlunoSemestre.TurnoCursoDescricao
	,0                                                                         -- Matric          
	,0                                                                         -- MatricEfetuada    
	,0                                                                         -- NaoRematriculados           
	,0                                                                         -- TotalCalourosMatriculados 
	,0                                                                         -- ABANDONO
	,0                                                                         -- FORMADOS
	,0                                                                         -- DESISTENTES  
	,COUNT(DBSecretariaAcademica.dbo.UvwConsultaAlunoSemestre.IdAluno)
	FROM DBSecretariaAcademica.dbo.UvwConsultaAlunoSemestre
 JOIN DBSecretariaAcademica.dbo.GradeConsepe
   ON DBSecretariaAcademica.dbo.GradeConsepe.IdGradeConsepe = DBSecretariaAcademica.dbo.UvwConsultaAlunoSemestre.IdGradeConsepe
 JOIN DBComum.dbo.Gpa
   ON DBComum.dbo.Gpa.IdGpa = DBSecretariaAcademica.dbo.GradeConsepe.IdGpa
 JOIN DBSecretariaAcademica.dbo.TipoAcessoIES
   ON DBSecretariaAcademica.dbo.TipoAcessoIES.IdTipoAcessoIES = DBSecretariaAcademica.dbo.UvwConsultaAlunoSemestre.IdTipoAcessoIES
WHERE DBSecretariaAcademica.dbo.UvwConsultaAlunoSemestre.IdCampus               = @IdCampus
  AND DBSecretariaAcademica.dbo.UvwConsultaAlunoSemestre.IdPeriodoLetivo        = @IdPeriodoLetivo
  AND DBSecretariaAcademica.dbo.UvwConsultaAlunoSemestre.SituacaoAcademicaAtivo = 1
  AND ((DBSecretariaAcademica.dbo.TipoAcessoIES.Calouro                         = 0) 
OR EXISTS (SELECT 1 FROM DBSecretariaAcademica.dbo.UvwConsultaAlunoSemestre ALUNO_SERIE
	           WHERE ALUNO_SERIE.IdCampus               = DBSecretariaAcademica.dbo.UvwConsultaAlunoSemestre.IdCampus
	 		     AND ALUNO_SERIE.IdPeriodoLetivo       &lt;= @IdPeriodoAnterior
				 AND ALUNO_SERIE.SituacaoAcademicaAtivo = 1
                 AND ALUNO_SERIE.idAluno                = DBSecretariaAcademica.dbo.UvwConsultaAlunoSemestre.IdAluno))
AND NOT EXISTS (SELECT 1 FROM DBRelatorio.dbo.AlunoRelatorio
                    JOIN DBSecretariaAcademica.dbo.GradeConsepe
                      ON DBSecretariaAcademica.dbo.GradeConsepe.IdGradeConsepe = DBRelatorio.dbo.AlunoRelatorio.IdGradeConsepe
                    JOIN DBComum.dbo.Gpa
                      ON DBComum.dbo.Gpa.IdGpa = DBSecretariaAcademica.dbo.GradeConsepe.IdGpa
                   WHERE DBRelatorio.dbo.AlunoRelatorio.IdCampus               = @IdCampus
                     AND DBRelatorio.dbo.AlunoRelatorio.IdPeriodoLetivo        = @idPeriodoAnterior
                     AND DBRelatorio.dbo.AlunoRelatorio.SituacaoAcademicaAtivo = 1
                     AND DBRelatorio.dbo.AlunoRelatorio.IdAluno                = DBSecretariaAcademica.dbo.UvwConsultaAlunoSemestre.IdAluno                 
AND EXISTS (SELECT 1 FROM DBSecretariaAcademica.dbo.UvwConsultaAlunoSemestre ALS
                                     JOIN DBSecretariaAcademica.dbo.TipoAcessoIES
                                       ON DBSecretariaAcademica.dbo.TipoAcessoIES.IdTipoAcessoIES = ALS.IdTipoAcessoIES
                                    WHERE ALS.IdCampus               = @IdCampus
                                      AND ALS.IdPeriodoLetivo        = @IdPeriodoLetivo
                                      AND ALS.SituacaoAcademicaAtivo = 1
                                      AND ALS.IdAluno                = DBRelatorio.dbo.AlunoRelatorio.IdAluno)) 


   GROUP BY  DBComum.dbo.Gpa.Descricao
           	,DBSecretariaAcademica.dbo.UvwConsultaAlunoSemestre.CursoDescricao
		    ,DBSecretariaAcademica.dbo.UvwConsultaAlunoSemestre.TurnoCursoDescricao 
   ORDER BY  DBComum.dbo.Gpa.Descricao
			,DBSecretariaAcademica.dbo.UvwConsultaAlunoSemestre.CursoDescricao
			,DBSecretariaAcademica.dbo.UvwConsultaAlunoSemestre.TurnoCursoDescricao

