  AND DBSecretariaAcademica.dbo.UvwConsultaAlunoSemestre.SituacaoAcademicaAtivo = 1
  AND ((DBSecretariaAcademica.dbo.TipoAcessoIES.Calouro                         = 0) 
OR EXISTS (SELECT 1 FROM DBSecretariaAcademica.dbo.UvwConsultaAlunoSemestre ALUNO_SERIE
	           WHERE ALUNO_SERIE.IdCampus               = DBSecretariaAcademica.dbo.UvwConsultaAlunoSemestre.IdCampus
