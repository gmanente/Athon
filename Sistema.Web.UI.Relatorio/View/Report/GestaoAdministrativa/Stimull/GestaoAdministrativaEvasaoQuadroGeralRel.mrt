-- 5 - CAMPO MATRICULA EFETUADA       
---------------------------------------------------------------------------------------------------
INSERT INTO @tabela
SELECT
	DBComum.dbo.Gpa.Descricao                     -- Descrição do Curso
	,DBRelatorio.dbo.AlunoRelatorio.CursoNome      -- Descrição do Curso
	,DBRelatorio.dbo.AlunoRelatorio.TurnoNome      -- Descricao do Turno
	,0                                             -- Matric   
	,0                                             -- Não Rematriculado      
	,COUNT(DBRelatorio.dbo.AlunoRelatorio.IdAluno) -- MatricEfetuada              
	,0                                             -- TotalCalourosMatriculados  
	,0
	,0
	,0  
	,0
	FROM DBRelatorio.dbo.AlunoRelatorio
 JOIN DBSecretariaAcademica.dbo.GradeConsepe
   ON DBSecretariaAcademica.dbo.GradeConsepe.IdGradeConsepe = DBRelatorio.dbo.AlunoRelatorio.IdGradeConsepe
 JOIN DBComum.dbo.Gpa
   ON DBComum.dbo.Gpa.IdGpa = DBSecretariaAcademica.dbo.GradeConsepe.IdGpa
WHERE DBRelatorio.dbo.AlunoRelatorio.IdCampus               = @IdCampus
  AND DBRelatorio.dbo.AlunoRelatorio.IdPeriodoLetivo        = @idPeriodoAnterior
  AND DBRelatorio.dbo.AlunoRelatorio.SituacaoAcademicaAtivo = 1
AND EXISTS (SELECT 1 FROM DBSecretariaAcademica.dbo.UvwConsultaAlunoSemestre
                    JOIN DBSecretariaAcademica.dbo.TipoAcessoIES
                      ON DBSecretariaAcademica.dbo.TipoAcessoIES.IdTipoAcessoIES = DBSecretariaAcademica.dbo.UvwConsultaAlunoSemestre.IdTipoAcessoIES
				   WHERE DBSecretariaAcademica.dbo.UvwConsultaAlunoSemestre.IdCampus               = @IdCampus
				     AND DBSecretariaAcademica.dbo.UvwConsultaAlunoSemestre.IdPeriodoLetivo        = @IdPeriodoLetivo
					 AND DBSecretariaAcademica.dbo.UvwConsultaAlunoSemestre.SituacaoAcademicaAtivo = 1
					 AND DBSecretariaAcademica.dbo.UvwConsultaAlunoSemestre.IdAluno                = DBRelatorio.dbo.AlunoRelatorio.IdAluno)
GROUP BY DBComum.dbo.Gpa.Descricao
	    ,DBRelatorio.dbo.AlunoRelatorio.CursoNome 
        ,DBRelatorio.dbo.AlunoRelatorio.TurnoNome 
ORDER BY DBComum.dbo.Gpa.Descricao
	    ,DBRelatorio.dbo.AlunoRelatorio.CursoNome 
        ,DBRelatorio.dbo.AlunoRelatorio.TurnoNome 

