 JOIN DBComum.dbo.Gpa
   ON DBComum.dbo.Gpa.IdGpa = DBSecretariaAcademica.dbo.GradeConsepe.IdGpa
 JOIN DBSecretariaAcademica.dbo.TipoAcessoIES
   ON DBSecretariaAcademica.dbo.TipoAcessoIES.IdTipoAcessoIES = DBSecretariaAcademica.dbo.UvwConsultaAlunoSemestre.IdTipoAcessoIES
