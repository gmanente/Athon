    <meta name="copyright" content="© <% DateTime ano = DateTime.Now; Response.Write(ano.ToString("yyyy")); %> | Todos os direitos reservados"/>
    <meta name="document-type" content="Private"/>
    <meta name="document-distribution" content="IU"/>
