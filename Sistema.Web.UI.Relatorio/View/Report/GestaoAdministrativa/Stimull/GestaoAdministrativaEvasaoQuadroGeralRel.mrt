        <SqlCommand>--DECLARE @idPeriodoLetivo   INT = 26
DECLARE @IdCampus          INT = 1
DECLARE @idPeriodoAnterior INT = (@idPeriodoLetivo - 1)

DECLARE @tabela TABLE
(
     Area                         VARCHAR(200) -- Area de Conhecimento
    ,Curso                        VARCHAR(200) -- Descrição do Curso
    ,Turno                        VARCHAR(200) -- Descricao do Turno
    ,Matric                       INT          -- Matric           
    ,NaoRematric                  INT          -- Não Matric    
    ,MatricEfetuada               INT          -- MatricEfetuada            
    ,TotalCalourosMatriculados    INT          -- TotalCalourosMatriculados  
    ,Abandono                     INT          -- Abandono
    ,Formado                      INT          -- Formados                    
    ,Desistente                   INT          -- Desistentes  
    ,Regularizado                 INT          -- Reularizado  
)

DECLARE @PeriodoLetivoBase     VARCHAR(6)
DECLARE @PeriodoLetivoAnterior VARCHAR(6)



---------------------------------------------------------------------------------------------------
-- MATRICULADOS SEMESTRE ANTERIOR
---------------------------------------------------------------------------------------------------
-- 1 - CAMPO MATRIC       
---------------------------------------------------------------------------------------------------
INSERT INTO @tabela
SELECT
	DBComum.dbo.Gpa.Descricao                     -- Descrição do Curso
	,DBRelatorio.dbo.AlunoRelatorio.CursoNome      -- Descrição do Curso
	,DBRelatorio.dbo.AlunoRelatorio.TurnoNome      -- Descricao do Turno
	,COUNT(DBRelatorio.dbo.AlunoRelatorio.IdAluno) -- Matric    
	,0                                             -- Não Rematriculadas     
	,0                                             -- MatricEfetuada              
	,0                                             -- TotalCalourosMatriculados
	,0
	,0
	,0 
	,0 
	FROM DBRelatorio.dbo.AlunoRelatorio
	LEFT JOIN DBSecretariaAcademica.dbo.GradeConsepe
	ON DBSecretariaAcademica.dbo.GradeConsepe.IdGradeConsepe = DBRelatorio.dbo.AlunoRelatorio.IdGradeConsepe
LEFT JOIN DBComum.dbo.Gpa
       ON DBComum.dbo.Gpa.IdGpa = DBSecretariaAcademica.dbo.GradeConsepe.IdGpa
    WHERE DBRelatorio.dbo.AlunoRelatorio.IdCampus             = 1
      AND DBRelatorio.dbo.AlunoRelatorio.IdPeriodoLetivo        = @idPeriodoAnterior
      AND DBRelatorio.dbo.AlunoRelatorio.SituacaoAcademicaAtivo = 1
 GROUP BY DBComum.dbo.Gpa.Descricao
         ,DBRelatorio.dbo.AlunoRelatorio.CursoNome
         ,DBRelatorio.dbo.AlunoRelatorio.TurnoNome
 ORDER BY DBComum.dbo.Gpa.Descricao
         ,dbrelatorio.dbo.alunorelatorio.cursonome
         ,dbrelatorio.dbo.alunorelatorio.turnonome
